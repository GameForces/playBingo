<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.playbingo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".rating"></activity>
        <activity android:name=".myprofile" />
        <activity
            android:name=".soundbool"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".findfriends"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".dailogloading"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".groupchat"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".onlinegame"
            android:launchMode="singleInstance"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".playwithfriends"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".RegisterActivity"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".SplashScreenActivity"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="sensorPortrait" />
    </application>

</manifest>