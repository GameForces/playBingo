        android:layout_width="200dp"
        android:layout_height="200dp"
        android:src="@drawable/o"
        android:layout_marginTop="55dp"
        android:visibility="invisible"
        />


    <RelativeLayout

        android:padding="9dp"
        android:background="@drawable/mainlayoutback"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="50dp"
        android:id="@+id/playonlineview"
        android:layout_width="130dp"
        android:layout_height="100dp">
        <ImageView
            android:id="@+id/maini1"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/playback"
            />

        <TextView
            android:layout_below="@id/maini1"
            android:id="@+id/play_online"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:fontFamily="cursive"
            android:shadowColor="@android:color/white"
            android:shadowDx="2.0"
            android:shadowDy="2.0"
            android:shadowRadius="1.0"
            android:text="Play Online"
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:textColorHint="#FFFFFF"
            android:textSize="15dp"
            android:textStyle="bold" />

    </RelativeLayout>
    <RelativeLayout

        android:padding="10dp"
        android:background="@drawable/mainlayoutback"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="50dp"
        android:layout_toRightOf="@+id/playonlineview"
        android:id="@+id/playwithfriendsview"
        android:layout_width="130dp"
        android:layout_height="100dp">


        <ImageView
            android:id="@+id/maini2"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/playfriends"
            />
        <TextView
            android:layout_below="@+id/maini2"
            android:id="@+id/play_with_friends"
            android:layout_width="400dp"
            android:layout_centerHorizontal="true"
            android:layout_height="120dp"
            android:textAlignment="center"
            android:fontFamily="cursive"
            android:shadowColor="@android:color/white"
            android:shadowDx="2.0"
            android:shadowDy="2.0"
            android:shadowRadius="10.0"
            android:text="Play with Friends"
            android:textColor="#ffffff"
            android:textSize="15dp"
            android:textStyle="bold" />


    </RelativeLayout>
    <RelativeLayout

        android:padding="10dp"

        android:background="@drawable/mainlayoutback"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="50dp"
        android:layout_below="@id/playonlineview"
        android:id="@+id/chatroomview"
        android:layout_width="130dp"
        android:layout_height="100dp">

        <ImageView
            android:id="@+id/maini3"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/chatroom"
            />
        <TextView
            android:layout_below="@+id/maini3"
            android:textColor="#ffffff"
            android:fontFamily="cursive"
            android:textSize="15dp"
            android:text="Chat Room"
            android:shadowColor="@android:color/white"
            android:shadowDx="2.0"
            android:shadowDy="2.0"
            android:shadowRadius="10.0"
            android:layout_width="400dp"
            android:textAlignment="center"
            android:layout_height="100dp"
            android:textStyle="bold"
            />
    </RelativeLayout>

    <RelativeLayout

        android:padding="10dp"
        android:background="@drawable/mainlayoutback"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="50dp"
        android:layout_toRightOf="@+id/chatroomview"
        android:layout_below="@id/playwithfriendsview"
        android:id="@+id/checkuser"
        android:layout_width="130dp"
        android:layout_height="100dp">

        <ImageView
            android:id="@+id/maini4"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/findfriends"
            />
        <TextView
            android:layout_below="@+id/maini4"
            android:textColor="#ffffff"
            android:textAlignment="center"
            android:fontFamily="cursive"
            android:textSize="15dp"
            android:text="Check Friends"
            android:layout_centerHorizontal="true"
            android:shadowColor="@android:color/white"
            android:shadowDx="2.0"
            android:shadowDy="2.0"
            android:shadowRadius="10.0"
            android:layout_width="400dp"
            android:layout_height="100dp"
            android:textStyle="bold"
            />
    </RelativeLayout>


</RelativeLayout>